{
  "name": "ebb-and-bloom",
  "version": "1.1.0",
  "description": "Governor-based world exploration engine and game",
  "type": "module",
  "main": "./engine/index.ts",
  "scripts": {
    "gen": "tsx gen/cli.ts",
    "gen:textures": "tsx gen/cli.ts textures",
    "gen:sdf-test": "tsx gen/cli.ts sdf-test",
    "gen:materials": "tsx gen/cli.ts materials",
    "dev": "vite --host",
    "dev:profile": "node --expose-gc scripts/profile-memory-leaks.cjs",
    "dev:heap": "node --heap-prof --heap-prof-interval=5000000 ./node_modules/vite/bin/vite.js --host",
    "analyze:heap": "node scripts/analyze-heap-snapshot.cjs",
    "build": "vite build",
    "preview": "vite preview --host --port 5173",
    "preview:build": "pnpm run build && vite preview --host --port 5173",
    "build:android": "pnpm run build && npx cap sync android && npx cap open android",
    "build:ios": "pnpm run build && npx cap sync ios && npx cap open ios",
    "sync": "npx cap sync",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:browser": "playwright test",
    "test:browser:prod": "PREVIEW=1 playwright test",
    "test:browser:ui": "playwright test --ui",
    "test:browser:codegen": "playwright codegen http://localhost:5173",
    "test:browser:report": "playwright show-report",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:codegen": "playwright codegen http://localhost:5173",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "pnpm run test && pnpm run test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "quality": "pnpm run type-check && pnpm run lint && pnpm run format:check && pnpm run test",
    "quality:fix": "pnpm run lint:fix && pnpm run format",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "upgrade": "pnpm update --latest",
    "upgrade:interactive": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "@capacitor-community/app-react": "0.1.0",
    "@capacitor-community/device-react": "0.1.0",
    "@capacitor-community/filesystem-react": "0.1.0",
    "@capacitor/app": "^7.1.0",
    "@capacitor/core": "^7.4.4",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/motion": "7.0.1",
    "@capacitor/preferences": "^7.0.2",
    "@dimforge/rapier3d-compat": "0.19.3",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@jbcom/strata": "^1.4.10",
    "@mui/icons-material": "7.3.5",
    "@mui/material": "7.3.5",
    "@react-spring/three": "10.0.3",
    "@react-spring/web": "10.0.3",
    "@react-three/cannon": "6.6.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@react-three/postprocessing": "^3.0.4",
    "@react-three/rapier": "^2.2.0",
    "@recast-navigation/three": "0.43.0",
    "@stdlib/stats": "0.3.3",
    "@types/uuid": "10.0.0",
    "@use-gesture/react": "10.3.1",
    "astronomy-engine": "2.1.19",
    "chemicaltools": "1.2.22",
    "d3-force-3d": "3.0.6",
    "fast-simplex-noise": "^4.0.0",
    "happy-dom": "20.0.10",
    "leva": "^0.10.1",
    "mathjs": "15.1.0",
    "miniplex": "2.0.0",
    "miniplex-react": "2.0.1",
    "nipplejs": "0.10.2",
    "odex": "3.0.0-rc.4",
    "pino": "10.1.0",
    "postprocessing": "6.38.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router-dom": "^7.9.5",
    "recast-navigation": "0.43.0",
    "seedrandom": "^3.0.5",
    "simple-statistics": "^7.8.8",
    "simplex-noise": "^4.0.3",
    "three": "^0.182.0",
    "three-mesh-bvh": "0.9.2",
    "three-stdlib": "2.36.1",
    "troika-three-text": "0.52.4",
    "uuid": "13.0.0",
    "yuka": "^0.7.8",
    "zod": "^3.24.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^2.0.65",
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@playwright/test": "^1.47.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.10.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/seedrandom": "^3.0.8",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "2.1.9",
    "ai": "^5.0.92",
    "axios": "^1.13.2",
    "commander": "^12.1.0",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "google-fonts-helper": "^3.7.3",
    "openai": "^6.8.1",
    "prettier": "3.6.2",
    "rollup-plugin-visualizer": "6.0.5",
    "sharp": "^0.34.5",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "engine",
    "simulation",
    "procedural",
    "law-based",
    "react-three-fiber",
    "r3f",
